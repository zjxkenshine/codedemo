<ehcache name="spring_boot_shiro_ehcache">
    <diskStore path="java.io.tmpdir"/>
    <defaultCache
            maxElementsInMemory="100000"
            eternal="false"
            overflowToDisk="true"
            timeToIdleSeconds="0"
            timeToLiveSeconds="0"
            diskPersistent="false"
            diskExpiryThreadIntervalSeconds="120"/>
</ehcache>