<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.kenshine</groupId>
    <artifactId>springboot-demos03</artifactId>
    <version>1.0-SNAPSHOT</version>
    <modules>
        <module>springbootdemo200-Lz4</module>
        <module>springbootdemo201-JCommander</module>
        <module>springbootdemo202-SBE</module>
        <module>springbootdemo203-JsonPath</module>
        <module>springbootdemo204-TestNG</module>
        <module>springbootdemo205-ExtentReport</module>
        <module>springbootdemo206-MapDB</module>
        <module>springbootdemo207-Aether</module>
        <module>springbootdemo208-Trove4j</module>
        <module>springbootdemo209-TypeSafeConfig</module>
        <module>springbootdemo210-Lightbend-Emoji</module>
        <module>springbootdemo211-JavaEWAH</module>
        <module>springbootdemo212-RoaringBitmap</module>
        <module>springbootdemo213-Vavr</module>
        <module>springbootdemo214-CGLib</module>
        <module>springbootdemo215-JavaParser</module>
        <module>springbootdemo216-Javacc</module>
        <module>springbootdemo217-FastUtil</module>
        <module>springbootdemo218-Koloboke</module>
        <module>springbootdemo219-SpringShell</module>
    </modules>

    <packaging>pom</packaging>

    <name>springboot-demos03</name>
    <description>springboot(java)各种整合各种技术demo(200~299)</description>
    <properties>
        <java.version>1.8</java.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <spring-boot.version>2.3.7.RELEASE</spring-boot.version>
    </properties>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.3.7.RELEASE</version>
        <relativePath/>
    </parent>

    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
        </dependency>

        <!--lombok-->
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
        </dependency>

        <!--配置文件提示-->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-configuration-processor</artifactId>
            <optional>true</optional>
        </dependency>

    </dependencies>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-dependencies</artifactId>
                <version>2.3.7.RELEASE</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
        </dependencies>
    </dependencyManagement>


    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.8.1</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                    <encoding>UTF-8</encoding>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
        </plugins>
    </build>

</project>