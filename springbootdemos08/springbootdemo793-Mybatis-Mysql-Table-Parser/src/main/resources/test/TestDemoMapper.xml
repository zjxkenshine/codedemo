<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kenshine.tableparser.TestMapper">

    <select id="selectList2" resultType="com.kenshine.tableparser.TestBean">
        SELECT * FROM test_demo
    </select>

    <select id="selectList" resultType="com.kenshine.tableparser.TestBean">
        SELECT * FROM test_demo02
    </select>

    <delete id="deleteById">
        DELETE FROM test_demo01 WHERE id = #{id}
    </delete>

    <insert id="insertDemo">
        INSERT INTO test_demo01(id,name,age,sex,create_time,update_time)
        VALUES(#{id},#{name},#{age},#{sex},#{createTime},#{updateTime})
    </insert>
</mapper>
